{
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "testEnvironment": "jsdom",
  "collectCoverageFrom": [
	"src/components/**/*.{js,jsx}",
	"!src/components/**/*.story.jsx",
	"!src/components/index.js"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "identity-obj-proxy",
    "@components": "<rootDir>/src/components"
  }
}